CREATE TABLE COURSE (COURSEID VARCHAR2(1000) NOT NULL, COURSENAME VARCHAR2(20), COURSEBLDG VARCHAR2(100), COURSEROOM VARCHAR2(1000), COURSECAPACITY NUMBER(38), PRIMARY KEY (COURSEID));

CREATE TABLE INSTRUCTOR (INSTRID VARCHAR2(1000), INSTRNAME VARCHAR2(50), INSTPREFIX VARCHAR2(50), INSTROFFICE VARCHAR2(50), INSTRDEPT VARCHAR2(50), INSTREMAIL VARCHAR2(100));


CREATE TABLE STUDENT (STUDENTID NUMBER(38) NOT NULL, STUDENTNAME VARCHAR2(50), STUDENTYEAR VARCHAR2(20), STUDENTMAJOR VARCHAR2(25), STUDENTGPA NUMBER(38), STUDENTEMAIL VARCHAR2(50), PRIMARY KEY (STUDENTID));


CREATE TABLE STUDENTENROLLMENT (COURSEID VARCHAR2(1000) NOT NULL, STUDENTID NUMBER(38) NOT NULL, PRIMARY KEY (COURSEID, STUDENTID));

	



